<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:controller="view.AdminViewController" prefHeight="312.0" prefWidth="568.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label layoutX="106.0" layoutY="14.0" text="Users">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <ListView fx:id="lv_users" layoutX="21.0" layoutY="40.0" prefHeight="256.0" prefWidth="218.0" />
      <Label layoutX="347.0" layoutY="14.0" text="Admin">
         <font>
            <Font size="43.0" />
         </font>
      </Label>
      <TextField fx:id="txt_username" layoutX="325.0" layoutY="85.0" promptText="Username">
         <font>
            <Font size="16.0" />
         </font></TextField>
      <Button fx:id="btn_delete_user" layoutX="325.0" layoutY="168.0" mnemonicParsing="false" onAction="#deleteUser" prefHeight="27.0" prefWidth="161.0" text="Delete User" />
      <Button fx:id="btn_create_user" layoutX="325.0" layoutY="128.0" mnemonicParsing="false" onAction="#createUser" prefHeight="27.0" prefWidth="161.0" text="Create User" />
      <Button fx:id="btn_logout" layoutX="494.0" layoutY="271.0" mnemonicParsing="false" onAction="#logout" text="Logout" />
   </children>
</AnchorPane>
